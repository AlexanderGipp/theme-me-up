<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ThemeMeUp.Mobile.MainPage">

    <StackLayout>
        <Frame BackgroundColor="#B32800" Padding="24" CornerRadius="0">
            <Label Text="Theme Me Up" HorizontalTextAlignment="Center" TextColor="White" FontSize="36"/>
        </Frame>
        <Button Text="Execute UseCase" Clicked="Execute_Clicked" />
        <Button Text="Print Presenter" Clicked="Resolve_Clicked" />
        <Label x:Name="labelDebug" Text="..." FontSize="16" Padding="30,0,30,0"/>
        <Image x:Name="imageDebug" />
    </StackLayout>

</ContentPage>
